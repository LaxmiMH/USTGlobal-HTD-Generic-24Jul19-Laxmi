stored procedure

DELIMITER $$

DROP PROCEDURE IF EXISTS `ust`.`emp`$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `emp`()
BEGIN
SELECT * from employee_info where deptid=10;
    END$$

DELIMITER ;

to call that
call emp;